{
  "extends": "next/core-web-vitals",
  "plugins": [
    "import",
    "object-pattern-newline",
    "react",
    "sort",
    "unused-imports"
  ],
  "rules": {

    // Set errors
    "no-var": [2],
    "react/jsx-props-no-spreading": [2],

    // Set lines
    "no-multiple-empty-lines": [1, { "max": 1 }],
    "padded-blocks": [1],
    "padding-line-between-statements": [1, {
      "blankLine": "always",
      "prev": "*",
      "next": ["block-like", "return", "export"]
    }],

    // Set imports
    "import/first": [1],
    "unused-imports/no-unused-imports": [1],
    "import/order": [1, {
      "newlines-between": "always",
      "groups": ["type", "builtin", "external", "sibling", "parent"],
      "pathGroups": [{
        "pattern": "\\./hooks/*",
        "group": "sibling",
        "position": "after"
      }, {
        "pattern": "\\./api/*",
        "group": "sibling",
        "position": "after"
      }, {
        "pattern": "\\./ui/*",
        "group": "sibling",
        "position": "after"
      }]
    }],

    // Set objects
    "object-pattern-newline/enforce": [1, { "items": 1 }],
    "object-curly-newline": [1, {
      "ImportDeclaration": "never",
      "ObjectExpression": { "minProperties": 2 },
      "ObjectPattern": { "minProperties": 2 },
      "ExportDeclaration": { "minProperties": 2 }
    }],

    // Set jsx
    "react/jsx-props-no-multi-spaces": [1],
    "react/jsx-max-props-per-line": [1],
    "jsx-quotes": [1, "prefer-single"],
    "react/jsx-tag-spacing": [1, { "beforeSelfClosing": "never" }],
    "react/self-closing-comp": [1],
    "react/jsx-boolean-value": [1, "always"],
    "react/jsx-closing-bracket-location": [1, "line-aligned"],
    "react/jsx-curly-brace-presence": [1, "never"],
    "react/jsx-curly-spacing": [1, "never"],
    "react/jsx-equals-spacing": [1, "never"],
    "react/jsx-first-prop-new-line": [1, "multiline-multiprop"],
    "react/jsx-no-useless-fragment": [1],
    "react/jsx-sort-props": [1, { "callbacksLast": true }]
    // "react/jsx-one-expression-per-line": [1, { "allow": "single-child" }]

    // "react/jsx-newline": [1, { "prevent": true }]
    // "react/jsx-fragments": [1, "element"]
